# Preparation.
=> DBGInvoke __enable_schema_sync_service('true')

=> DBGInvoke __drop_tidb_table(default, test)
=> drop table if exists default.test

=> DBGInvoke __set_flush_threshold(1000000, 1000000)

# Data.
=> DBGInvoke __mock_tidb_table(default, test, 'a int, b float, c decimal(4, 3), d char, e MyDatetime(5)')
=> DBGInvoke __refresh_schemas()
=> DBGInvoke __put_region(4, 0, 100, default, test)
=> DBGInvoke __raft_insert_row(default, test, 4, 50,    1, 1.234, 1.234, '123d', '2021-01-12 01:23:45.114518')
=> DBGInvoke __raft_insert_row(default, test, 4, 51,    -1, -1.234, -1.234, 'd123', '20200202200220.022002')
=> DBGInvoke __raft_insert_row(default, test, 4, 52,    0, 0, 0, '', '2021-01-12')

=> DBGInvoke dag('select count(1) from default.test group by cast_int_string(a), cast_real_string(b), cast_decimal_string(c), cast_string_string(d), cast_time_string(e)', 4, 'encode_type:chunk')
┌─count(1)─┬─cast_int_string(a)─┬─cast_real_string(b)─┬─cast_decimal_string(c)─┬─cast_string_string(d)─┬─cast_time_string(e)───────┐
│        1 │ 1                  │ 1.234               │ 1.234                  │ 123d                  │ 2021-01-12 01:23:45.11451 │
│        1 │ 0                  │ 0                   │ 0.000                  │                       │ 2021-01-12 00:00:00.00000 │
│        1 │ -1                 │ -1.234              │ -1.234                 │ d123                  │ 2020-02-02 20:02:20.02200 │
└──────────┴────────────────────┴─────────────────────┴────────────────────────┴───────────────────────┴───────────────────────────┘

# Clean up.
=> DBGInvoke __drop_tidb_table(default, test)
=> drop table if exists default.test
